      "source": [
        "def fact(n):\n",
        "  if (n==0 or n==1):\n",
        "    return 1\n",
        "  else:\n",
        "    return n*fact(n-1)\n",
        "num=6\n",
        "print(\"The factorial of\",num,\"is:\",fact(num))"
      ]
    }
